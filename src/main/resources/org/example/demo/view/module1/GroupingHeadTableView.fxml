<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import java.net.URL?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="org.example.demo.view.module1.GroupingHeadTableView"
      id="center-tab-pane"
      alignment="CENTER"
      spacing="5">
    <HBox alignment="CENTER" spacing="5" VBox.vgrow="ALWAYS">
        <TableView fx:id="tableView" HBox.hgrow="ALWAYS">
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
            <columns>
                <TableColumn text="other">
                    <columns>
                        <TableColumn fx:id="column1" text="column1"/>
                        <TableColumn text="other">
                            <columns>
                                <TableColumn fx:id="column2" text="column2"/>
                                <TableColumn fx:id="column3" text="column3"/>
                            </columns>
                        </TableColumn>
                    </columns>
                </TableColumn>
            </columns>
        </TableView>
    </HBox>
    <stylesheets>
        <URL value="@/org/example/demo/css/GroupingHeadTableView.css"/>
    </stylesheets>
</VBox>

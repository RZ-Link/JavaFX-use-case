<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="org.example.demo.view.feedback.TreeViewDialog"
            prefWidth="400">
    <padding>
        <Insets top="10.0" right="10.0" bottom="10.0" left="10.0"/>
    </padding>
    <center>
        <VBox alignment="CENTER_LEFT" spacing="5">
            <HBox alignment="CENTER_LEFT">
                <TreeView fx:id="treeView" HBox.hgrow="ALWAYS">
                </TreeView>
            </HBox>
            <padding>
                <Insets top="5" bottom="5"/>
            </padding>
        </VBox>
    </center>
    <bottom>
        <HBox alignment="CENTER_RIGHT" spacing="5">
            <Button fx:id="confirmButton" prefHeight="32.0" prefWidth="64.0" text="确定"/>
            <Button fx:id="cancelButton" prefHeight="32.0" prefWidth="64.0" text="取消"/>
        </HBox>
    </bottom>
</BorderPane>
